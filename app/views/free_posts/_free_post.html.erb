<div id="<%= dom_id free_post %>">
  <p>
    <strong>User:</strong>
    <%= free_post.user_id %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= free_post.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= free_post.body %>
  </p>

  <p>
    <strong>Icon:</strong>
    <%= free_post.icon %>
  </p>

  <p>
    <strong>Created at:</strong>
    <%= free_post.created_at %>
  </p>

  <p>
    <strong>Updated at:</strong>
    <%= free_post.updated_at %>
  </p>

</div>
